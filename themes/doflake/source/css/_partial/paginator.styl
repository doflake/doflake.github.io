.paginator {
  .paginator-prev-a {
    border: none;
    background-color: #ffffff
    box-shadow: 0 8px 15px rgba(0,0,0,0.05), 0 0 1px rgba(0,0,0,0.1);
    padding: 2px 2px 2px 2px;

    position: relative;
    top: 10px;
    left: 5px; 

    color: rgb(50,50,50);
    font-style: normal;
    text-decoration-line: none;
    text-align: center;
    }
  .paginator-next-a {
    border: none;
    background-color: #ffffff
    box-shadow: 0 8px 15px rgba(0,0,0,0.05), 0 0 1px rgba(0,0,0,0.1);
    padding: 2px 2px 2px 2px;

    position: relative;
    top: 10px;
    right: 5px; 
    bottom: 20px;
    margin-bottom: 50px;

    color: rgb(50,50,50);
    font-style: normal;
    text-decoration-line: none;
    text-decoration-color: #ffffff;
    text-align: center;
    }
}